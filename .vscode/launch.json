{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno: Run",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect-wait",
        "--allow-all",
        "${file}"
      ],
      "attachSimplePort": 9229
    },
    {
      "name": "Deno: Dev Server",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect-wait",
        "--allow-net",
        "--allow-env",
        "--watch",
        "src/index.deno.ts"
      ],
      "attachSimplePort": 9229,
      "outputCapture": "console"
    },
    {
      "name": "Deno: Test Current File",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "--inspect-wait",
        "--allow-all",
        "${file}"
      ],
      "attachSimplePort": 9229
    },
    {
      "name": "Deno: Test All",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "--inspect-wait",
        "--allow-all"
      ],
      "attachSimplePort": 9229
    },
    {
      "name": "Deno: Test Webhook",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "--inspect-wait",
        "--allow-net",
        "--allow-env",
        "tests/webhook.test.ts"
      ],
      "attachSimplePort": 9229
    }
  ]
}
